{
	"debug.toolBarLocation": "docked",
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.formatOnSave": true,
	"files.encoding": "utf8",
	"files.trimTrailingWhitespace": true,
	"files.eol": "\n",
	"files.associations": {
		"*.json": "jsonc",
		"*.desktop": "properties",
		".clang-format": "yaml"
	},
	"files.exclude": {
		"chalet_external": true
	},
	"terminal.integrated.env.windows": {
		"Path": "${workspaceFolder}/build/x86_64-w64-mingw32_Debug/chalet_external/SFML/lib;%Path%"
	}
}